<rdf:Description rdf:about="http://raul.deri.ie/forms/buyproduct#valueproduct_name">
	<rdf:subject>http://raul.deri.ie/forms/buyproduct#123</rdf:subject>

	<rdf:predicate>rdfs:label</rdf:predicate>
	<rdf:object>Sony Bravia LX900</rdf:object>
</rdf:Description>

<rdf:Description rdf:about="http://raul.deri.ie/forms/buyproduct#valueproduct_description">
	<rdf:subject>http://raul.deri.ie/forms/buyproduct#123</rdf:subject>
	<rdf:predicate>rdfs:comment</rdf:predicate>
	<rdf:object>LCD Television</rdf:object>

</rdf:Description>

<rdf:Description rdf:about="http://raul.deri.ie/forms/buyproduct#valueproduct_amount">
	<rdf:subject>http://raul.deri.ie/forms/buyproduct#123</rdf:subject>
	<rdf:predicate>gr:amountOfThisGood</rdf:predicate>
	<rdf:object></rdf:object>
</rdf:Description>

