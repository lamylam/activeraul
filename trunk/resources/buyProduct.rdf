	<id xmlns="http://purl.org/NET/raul#">maindiv</id>
	<widgets xmlns="http://purl.org/NET/raul#">http://raul.deri.ie/forms/buyproduct#buyProductContainer</widgets>
</rdf:Description>

<rdf:Description rdf:about="http://raul.deri.ie/forms/buyproduct#buyProductContainer">
	<rdf:type rdf:resource="http://purl.org/NET/raul#WidgetContainer"/>
	<method xmlns="http://purl.org/NET/raul#">http://raul.deri.ie/forms/buyproduct#postProduct</method>

	<id xmlns="http://purl.org/NET/raul#">buyProduct</id>
	<action xmlns="http://purl.org/NET/raul#"></action>
	<list xmlns="http://purl.org/NET/raul#">http://raul.deri.ie/forms/buyproduct#buyProductContainerList</list>
</rdf:Description>

<rdf:Description rdf:about="http://raul.deri.ie/forms/buyproduct#postProduct">
